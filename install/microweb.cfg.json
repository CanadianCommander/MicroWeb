{
  "general": {
    "TCPProtocol":        "tcp4",
    "TCPPort":            ":80",
    "staticDirectory":    "/var/www/",
    "logFile":            "/var/log/microweb.log",
    "logVerbosity":       "info",
    "autoReloadSettings": false
  },

  "tls": {
    "enableTLS": false
  },

  "tune": {
    "httpReadTimeout":      "100ms",
    "httpResponseTimeout":  "1s",
    "cacheTTL":             "360s"
  },

  "security": {
    "user":   "www-data",
    "strict": true
  }
}
