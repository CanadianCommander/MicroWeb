{
  "general": {
    "TCPProtocol":      "tcp4",
    "TCPPort":          ":80",
    "staticDirectory":  "./static/",
    "logFile":          "/var/log/microweb.log",
    "logVerbosity":     "info"
  },

  "tls": {
    "enableTLS": false
  },

  "tune": {
    "httpReadTimout":       "100ms",
    "httpResponseTimeout":  "1s",
    "cacheTTL":             "360s"
  }
}
