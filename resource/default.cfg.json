{
  "general": {
    "TCPProtocol":      "tcp4",
    "TCPPort":          ":8080",
    "staticDirectory":  "./exampleWeb/",
    "logFile":          "./web.log",
    "logVerbosity":     "info"
  },

  "tls": {
    "enableTLS": false,
    "certFile": "./resource/tlsExample/server.crt",
    "keyFile": "./resource/tlsExample/server.key"
  },

  "tune": {
    "httpReadTimout":       "100ms",
    "httpResponseTimeout":  "100ms",
    "cacheTTL":             "60s"
  },

  "plugin": {
    "plugins":
      [
        {
          "binding": "/template/test.gohtml",
          "plugin":"resource/plugin1/plugin1.so"
        },
        {
          "binding": "/template/index.gohtml",
          "plugin":"resource/plugin2/plugin2.so"
        }
      ]
  }
}
