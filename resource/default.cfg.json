{
  "general": {
    "TCPProtocol":      "tcp4",
    "TCPPort":          ":80",
    "staticDirectory":  "./static/",
    "logFile":          "/etc/microweb/microweb.log",
    "logVerbosity":     "info",
    "autoReloadSettings": false 
  },

  "tls": {
    "enableTLS": false
  },

  "tune": {
    "httpReadTimout":       "100ms",
    "httpResponseTimeout":  "1s",
    "cacheTTL":             "360s"
  }
}
